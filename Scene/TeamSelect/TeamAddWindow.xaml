<Window x:Class="FightingCommunityAdministrator.TeamAddWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="新規チームの追加" Height="104" Width="351" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button Content="キャンセル" Height="23" HorizontalAlignment="Right" Name="CancelButton" VerticalAlignment="Bottom" Width="105" TabIndex="1" Click="_ClickButton" IsCancel="True" Margin="0,0,3,3" />
        <Button Content="追加" Height="23" HorizontalAlignment="Right" Margin="0,0,112,3" Name="AddButton" VerticalAlignment="Bottom" Width="105" TabIndex="0" IsDefault="True" />
        <Label Content="追加するチーム名を入力してください(15文字まで)" Height="28" HorizontalAlignment="Left" Name="AddLabel" VerticalAlignment="Top" />
        <TextBox HorizontalAlignment="Stretch" Name="TeamNameTextBox" VerticalAlignment="Top" Text="ここに名前を入力してね" Margin="5,25,6,0" Height="23" />
    </Grid>
</Window>
